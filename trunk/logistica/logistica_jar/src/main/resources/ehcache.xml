<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />

	<defaultCache maxElementsInMemory="1000"
		maxElementsOnDisk="10000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="14400" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LRU" statistics="true">
	</defaultCache>

	<cache name="logistica.model.Cliente" maxElementsInMemory="100"
		maxElementsOnDisk="1000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="14400" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LRU" statistics="true">
	</cache>
	
	<cache name="logistica.model.Chofer" maxElementsInMemory="100"
		maxElementsOnDisk="1000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="14400" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LRU" statistics="true">
	</cache>
	
	<cache name="logistica.model.Configuracion" maxElementsInMemory="3"
		maxElementsOnDisk="10" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="14400" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LRU" statistics="true">
	</cache>
	
	<cache name="logistica.model.Localidad" maxElementsInMemory="300"
		maxElementsOnDisk="1000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="14400" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LRU" statistics="true">
	</cache>
	
	<cache name="logistica.model.Proveedor" maxElementsInMemory="100"
		maxElementsOnDisk="1000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="14400" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LRU" statistics="true">
	</cache>
	
	<cache name="logistica.model.Provincia" maxElementsInMemory="50"
		maxElementsOnDisk="100" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="14400" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LRU" statistics="true">
	</cache>
	
	<cache name="logistica.model.Usuario" maxElementsInMemory="100"
		maxElementsOnDisk="1000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="14400" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LRU" statistics="true">
	</cache>
	
	<cache name="logistica.model.Sucursal" maxElementsInMemory="20"
		maxElementsOnDisk="100" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="14400" timeToLiveSeconds="0"
		memoryStoreEvictionPolicy="LRU" statistics="true">
	</cache>
	

	<!-- Query cache -->
	<cache
		name="org.hibernate.cache.StandardQueryCache"
		maxElementsInMemory="1000"
		eternal="false"
		overflowToDisk="false"
		timeToIdleSeconds="14400"
		timeToLiveSeconds="0"
		diskPersistent="false"
		statistics="true" />
	<cache
		name="org.hibernate.cache.UpdateTimestampsCache"
		maxElementsInMemory="1000"
		eternal="false"
		overflowToDisk="false"
		timeToIdleSeconds="14400"
		timeToLiveSeconds="0"
		diskPersistent="false">
	</cache>
</ehcache>